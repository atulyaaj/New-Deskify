@model DeskEntity.Model.BookingSeat


@for (int i = 0; i < Convert.ToInt32(TempData["Count"]); i++) ;

<html>
    <head>
    <link rel="stylesheet" href="~/css/site2.css" />
    </head>
</html>

@if (Model.SeatStatus == 2)
{
    <div class="card-div">
        <div class="card p-2">
            <h4 class="mt-2"><img src="~/img/rejected.png" style="width:40px;height:40px">Your seat is cancelled</h4>
        </div>
    </div>
}
else
{
    <div class="card-div">
        <div class="card p-2">
            <h4 class="mt-2"><img src="~/img/approved.png" style="width:40px;height:40px">Your seat is booked</h4>
        </div>
    </div>
}


