@model DeskEntity.Model.BookingSeat

<form method="post">
    <div class="form-group">

        <label for="From Date">From Date</label>
        <input type="date" asp-for="FromDate" class="form-control" placeholder="Select the From Date" required />
    </div>
    <div class="form-group">

        <label for="To date">To date</label>
        <input type="date" asp-for="ToDate" class="form-control" placeholder="Select the To Date" required />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>